@page
@model List<Employee>

@{
    ViewData["Title"] = "Employee Data";
}

<div class="text-center">
    <h1 class="display-4">Employee Data Homepage</h1>
</div>

<div class="text-center">
    <table class="table table-border table-striped">
        <thead>
            <tr>
                <th>Last Name</th>
                <th>First Name</th>
                <th>Position</th>
                <th>Authorization</th>
                <th>Survey Sent</th>
                <th>Survey Submitted</th>
                <th>International</th>
                <th>Name Change Complete</th>
                <th>Semester</th>
                <th>Year</th>
                <th>Supervisor</th>
            </tr>
        </thead>
        <tbody>
                @foreach (var x in Model)

                {
                <tr>
                    <td>@x.Notes</td>
                </tr>
                }
            </tbody>
    </table>
    
</div>